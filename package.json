{
  "scripts": {
    "cleanup": "for file in $(find frontend public -type f -iname *.map 2>/dev/null); do rm $file ${file%.*}; done",
    "build": "npm run createfirebaseconf; npm run cleanup; NODE_ENV=production webpack -p; babel $(find frontend -type f -iname *.jsx) -s --retain-lines -d ./",
    "build:dev": "npm run createfirebaseconf; npm run cleanup; webpack -d",
    "build:watch": "npm run createfirebaseconf; npm run cleanup; webpack --watch",
    "createfirebaseconf": "firebase setup:web --json > ./frontend/firebase-config.json",
    "serve": "GCLOUD_PROJECT=- NODE_ENV=devserver npm run build:watch",
    "format": "prettier --write --single-quote --print-width=120 --parser=flow --tab-width=2 \"js/**/*.{js,jsx}\"",
    "lint": "eslint **/*.{js,jsx} --quiet",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:update": "jest -u"
  },
  "dependencies": {
